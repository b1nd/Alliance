<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<AnchorPane styleClass="pane" prefHeight="400.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.buzulukov.alliance.controllers.AccountsController">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ListView fx:id="accountsListView" styleClass="list-view-dialogs" focusTraversable="false"
                          prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
                <BorderPane styleClass="container" prefHeight="50.0" prefWidth="200.0">
                    <center>
                        <Button styleClass="button-main" mnemonicParsing="false" onAction="#onClose" text="OK"
                                BorderPane.alignment="CENTER"/>
                    </center>
                </BorderPane>
            </children>
        </VBox>
    </children>
</AnchorPane>
